CREATE TABLE userEntity(
    id INTEGER NOT NULL PRIMARY KEY AUTOINCREMENT,
    userName TEXT NOT NULL
);

getAllUsers:
SELECT *
FROM userEntity;

getUserById:
SELECT *
FROM userEntity
WHERE id = ?;

insertNote:
INSERT OR REPLACE
INTO userEntity(
    id,
    userName
) VALUES(?, ?);

deleteNoteById:
DELETE FROM userEntity
WHERE id = ?;